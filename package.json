  "scripts": {
    "clean": "lerna clean --yes && rm -rf node_modules",
    "vc:reset": "rm -rf .vercel && mkdir .vercel",
    "vc:components": "yarn vc:reset && cp ~/.vercel/woodbook.json .vercel/project.json",
    "format": "prettier --write \"./\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "dev:api": "lerna run dev --scope @modelbcompany/woodmont-api",
    "dev:components": "lerna run dev --scope @modelbcompany/woodmont-ui",
    "dev": "lerna run dev",
    "build:components": "lerna run build --scope @modelbcompany/woodmont-ui",
    "stage:api": "lerna run stage --scope @modelbcompany/woodmont-api",
    "stage:components": "yarn vc:components && vc -A woodbook.vercel.json",
    "prod:api": "lerna run prod --scope @modelbcompany/woodmont-api",
    "prod:components": "yarn stage:components --prod"
  },
  "dependencies": {
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-export-default-from": "^7.10.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/plugin-proposal-throw-expressions": "^7.10.1",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/plugin-transform-typescript": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.9",
    "@types/pino": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^7.1.0",
