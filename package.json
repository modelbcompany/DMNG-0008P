    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jest": "^26.0.1",
    "lerna": "^3.22.1",
    "lodash": "^4.17.15",
    "npm": "^6.14.5",
    "npm-run-all": "^4.1.5",
    "pino": "^6.3.2",
    "prettier": "^2.0.5",
