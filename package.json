{
  "name": "@modelbcompany/woodmont",
  "description": "[DMNG-0008P] WordPress site, React component library, and Serverless API for the Woodmont Landing Page project.",
  "keywords": [
    "component library",
    "microbundle",
    "react",
    "sass",
    "serverless functions",
    "storybook",
    "typescript",
    "vercel",
    "wordpress",
    "yarn"
  ],
  "homepage": "https://woodmontdev.wpengine.com",
  "repository": "git://github.com/modelbcompany/DMNG-0008P.git",
  "bugs": {
    "url": "https://github.com/modelbcompany/DMNG-0008P/issues",
    "email": "web@modelb.com"
  },
  "license": "UNLICENSED",
  "author": {
    "name": "Lexus Drumgold",
    "email": "lexus.drumgold@modelb.com",
    "url": "https://modelb.com"
  },
  "source": "src/index.ts",
  "main": "dist/woodmont.js",
  "module": "dist/woodmont.m.js",
  "esmodule": "dist/woodmont.modern.js",
  "unpkg": "dist/woodmont.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "directory": "dist"
  },
  "scripts": {
    "kill:process": "lsof -t -i tcp:3000 | xargs kill",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "formatlint": "run-s format lint",
    "dev:components": "start-storybook -p 3000 -s app/public/assets --ci",
    "dev:components:docs": "yarn dev --docs",
    "dev": "vc dev --listen 9000",
    "pretest": "NODE_ENV=test",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "prebundle": "rm -rf ./dist && tsc",
    "bundle": "microbundle --jsx React.createElement --sourcemap false --tsconfig tsconfig.prod.json",
    "postbundle": "cp -R src/sass dist",
    "prebuild:components": "rm -rf ./build",
    "build:components": "build-storybook -o build",
    "build:components:docs": "build-storybook -o build/docs --docs",
    "build": "run-s build:**",
    "postbuild:components": "cp -R app/public/assets build",
    "postbuild:components:docs": "cp -R app/public/assets build/docs",
    "vercel:dev": "run-p dev:*",
    "vercel:reset": "rm -rf .vercel"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
