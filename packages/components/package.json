  "main": "dist/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "directory": "dist"
  },
  "scripts": {
    "prebundle": "rm -rf ./dist",
    "bundle": "tsc --rootDir ./src",
    "postbundle": "cp -R src/sass dist",
    "bundle:dev": "tsc -w --rootDir ./src",
    "dev": "start-storybook -p 3000 -s assets --ci",
    "dev:docs": "yarn dev --docs",
    "pretest": "NODE_ENV=test",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "prebuild:canvas": "rm -rf ./build",
    "build": "run-s build:**",
    "build:canvas": "build-storybook -o build",
    "build:docs": "build-storybook -o build/docs --docs",
    "postbuild:canvas": "cp -R assets build",
    "postbuild:docs": "cp -R assets build/docs",
    "stage": "vc",
    "prod": "yarn stage --prod"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
