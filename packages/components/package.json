{
  "name": "@modelbcompany/woodmont-ui",
  "version": "1.0.0-alpha",
  "description": "Component library for the Woodmont Landing Page project.",
  "keywords": [
    "component library",
    "react",
    "sass",
    "storybook",
    "typescript"
  ],
  "homepage": "https://github.com/modelbcompany/DMNG-0008P/tree/master/packages/components",
  "repository": "git://github.com/modelbcompany/DMNG-0008P.git",
  "bugs": {
