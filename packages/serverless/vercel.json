{
  "version": 2,
  "scope": "modelb",
  "env": {
    "apiToken": "@apiToken",
    "companyCode": "@companyCode",
    "marketingAPIKey": "@marketingAPIKey",
    "propertyId": "@propertyId"
  },
  "public": true,
  "redirects": [
    {
      "source": "/repo",
      "destination": "https://github.com/modelbcompany/DMNG-0008P/tree/master/packages/serverless",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}